{
  "name": "crontab-ui-daemon",
  "version": "1.0.0",
  "description": "small package to run crontab-ui as a daemon",
  "scripts": {
    "start": "./node_modules/pm2/bin/pm2 start ecosystem.config.js",
    "stop": "./node_modules/pm2/bin/pm2 stop ecosystem.config.js",
    "restart": "./node_modules/pm2/bin/pm2 restart ecosystem.config.js",
    "reload": "./node_modules/pm2/bin/pm2 reload ecosystem.config.js",
    "status": "./node_modules/pm2/bin/pm2 status ecosystem.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fboldo/crontab-ui-daemon.git"
  },
  "keywords": [
    "crontab",
    "crontab-ui"
  ],
  "author": "Filipe Boldo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fboldo/crontab-ui-daemon/issues"
  },
  "homepage": "https://github.com/fboldo/crontab-ui-daemon#readme",
  "dependencies": {
    "crontab-ui": "^0.3.9",
    "dotenv": "^8.2.0",
    "pm2": "^4.2.3"
  }
}
